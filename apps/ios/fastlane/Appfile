# Appfile - app identifiers and team settings
# For more information: https://docs.fastlane.tools/advanced/Appfile/

# App Store Connect credentials
app_identifier("com.shadowlabs.trendsight")  # Bundle Identifier
apple_id(ENV["APPLE_ID"])                    # Your Apple Developer email
itc_team_id(ENV["ITC_TEAM_ID"])              # App Store Connect Team ID (optional)
team_id("6H6N882H4S")                        # Developer Portal Team ID

# All deployment lanes use the same App Store Connect app
# Different Xcode configurations (Debug/Staging/TestFlight/Release) can use
# different local bundle IDs for side-by-side installation during development,
# but App Store Connect only has one app: com.shadowlabs.trendsight
