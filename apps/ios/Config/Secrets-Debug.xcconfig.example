// Secrets-Debug.xcconfig.example
// Template for Debug (Local) environment secrets
//
// SETUP: Copy this file to Secrets-Debug.xcconfig and fill in your values

// Local Supabase instance (running via supabase start)
SUPABASE_URL = http:/$()/127.0.0.1:54321
SUPABASE_ANON_KEY = your_local_supabase_anon_key_here

// Local backend API (running via just dev-backend)
API_BASE_URL = http:/$()/localhost:8888/api/v1

// PostHog Analytics
POSTHOG_API_KEY = your_posthog_api_key_here
POSTHOG_HOST = https:/$()/us.i.posthog.com
