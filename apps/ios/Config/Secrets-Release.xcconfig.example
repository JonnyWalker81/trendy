// Secrets-Release.xcconfig.example
// Template for Production/Release environment secrets
//
// SETUP: Copy this file to Secrets-Release.xcconfig and fill in your values

// Production Supabase instance
SUPABASE_URL = https:/$()/your-project.supabase.co
SUPABASE_ANON_KEY = your_production_supabase_anon_key_here

// Production backend API (MUST include /api/v1 suffix)
API_BASE_URL = https:/$()/your-production-api.example.com/api/v1

// PostHog Analytics
POSTHOG_API_KEY = your_posthog_api_key_here
POSTHOG_HOST = https:/$()/us.i.posthog.com

// Google Sign-In (same Client ID can be used across environments)
GOOGLE_CLIENT_ID = your_google_ios_client_id.apps.googleusercontent.com
